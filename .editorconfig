# http://editorconfig.org
root = true

[*.{js,jsx,ts,tsx,scss,css,less,html,go,yml}]
charset = utf-8
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[package.json]
indent_style = space
indent_size = 2

[*.md]
trim_trailing_whitespace = false

[*.sql]
charset = utf-8
indent_style = tab
trim_trailing_whitespace = true
end_of_line = lf
indent_size = 8
insert_final_newline = false
